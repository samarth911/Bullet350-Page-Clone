$(function() {

    $(document).on('click','.faqCategoryAna ',function(){
      let ctaText = $(this).children("a").text().trim();
      let category = $(this).children("a").text().trim();
      let nextUrl = $(this).children("a").attr("href");
      let heading = $(nextUrl).children(".mainHeading").text().trim();
       if(nextUrl == undefined){
        nextUrl = "";
       }
    
       dataLayer.push({
        'event':'faq',
        'eventCategory':'faq',
        'eventAction':ctaText,
        'eventLabel':nextUrl,
        'headingName':heading,
        'category':category,
        'subCategory':""
    });
    });
    
    $(document).on('click','.popular_question .acc-tab',function(){
      let ctaText = $(this).children("span").text().trim();
      let category;
      let nextUrl = $(this).attr("href");
      let heading = $(this).parent(".popular_question").parent(".accordion").siblings(".mainHeading").text().trim();
       if(nextUrl == undefined){
        nextUrl = "";
       }
       $(".faqCategoryAna ").each(function(){
        let url = $(this).children("a").attr("class");
        if(url == "cat-nm active show"){
          category = $(this).text().trim()
        }
       });
    
       dataLayer.push({
        'event':'faq',
        'eventCategory':'faq',
        'eventAction':ctaText,
        'eventLabel':nextUrl,
        'headingName':heading,
        'category':category,
        'subCategory':""
    });
    });

    $(document).on('click','.popular_question .acc-content p a',function(){
        let ctaText = $(this).text().trim();
        let category;
        let nextUrl = $(this).attr("href");
        let heading = $(this).parent("p").parent(".acc-content").parent(".popular_question").parent(".accordion").siblings(".mainHeading").text().trim();
         if(nextUrl == undefined){
          nextUrl = "";
         }
         $(".faqCategoryAna ").each(function(){
          let url = $(this).children("a").attr("class");
          if(url == "cat-nm active show"){
            category = $(this).text().trim()
          }
         });
      
         dataLayer.push({
          'event':'faq',
          'eventCategory':'faq',
          'eventAction':ctaText,
          'eventLabel':nextUrl,
          'headingName':heading,
          'category':category,
          'subCategory':""
      });
      });
    
    });
/*FAQ's [START]*/
$(document).ready(function(){
$("#faq .nav-tabs li:nth-child(1) > a").addClass('active show');

$("#faq .tab-content .tab-pane:nth-child(1)").addClass('active show');

$("#faq .accordion li.popular_question:nth-child(1) .acc-content").show();

$("#faq ul.accordion > li .acc-tab").click(function() {
	$(this).closest('li').siblings().find('.acc-tab').removeClass('active').next('.acc-content').slideUp(250);
	$(this).toggleClass("active").next('.acc-content').slideToggle(250);
});


$('.categorie-holder > span').click(function() {
	$('.faq-lh .nav.nav-tabs').toggle();
});
var categories_value = $(this).find(".cat-nm.active.show").html();
$('.cat-vl').text(categories_value);

function selMob(){
	if ($(window).width() < 767) {

		$('.faq-lh .nav.nav-tabs li').click(function() {
			var categories_value = $(this).find(".cat-nm").text();
			$('.faq-lh .nav.nav-tabs').hide();
			$('.cat-vl').text(categories_value);
		});
	}
	else{
		$('.faq-lh .nav.nav-tabs').show();
	}
}
selMob();
$(window).resize(function(){
	selMob();
});
});
/*FAQ's [END]*/

